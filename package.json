  "scripts": {
    "clean": "rimraf dist",
    "compile": "better-npm-run compile",
    "compile:mobile": "better-npm-run compile:mobile",
    "compile:langs": "better-npm-run compile:langs",
    "lint": "eslint bin build config server src tests",
    "lint:fix": "npm run lint -- --fix",
    "lint:translations": "better-npm-run lint:translations",
    "start": "better-npm-run start",
    "dev": "better-npm-run dev",
    "test": "better-npm-run test",
    "test:dev": "npm run test -- --watch",
    "tx:push": "better-npm-run tx:push",
    "tx:pull": "better-npm-run tx:pull",
    "deploy": "better-npm-run deploy",
    "deploy:dev": "better-npm-run deploy:dev",
    "deploy:prod": "better-npm-run deploy:prod",
    "codecov": "cat coverage/*/lcov.info | codecov"
  },
  "betterScripts": {
    "compile": {
      "command": "node bin/compile",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "compile:mobile": {
      "command": "node bin/compile",
      "env": {
        "NODE_ENV": "mobile",
        "DEBUG": "app:*"
      }
    },
    "compile:langs": {
      "command": "babel bin/translate.js | node"
    },
    "dev": {
      "command": "nodemon bin/dev-server --ignore dist --ignore coverage --ignore tests --ignore src",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "npm run lint && npm run test && npm run clean && npm run compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy:dev": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy:prod": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "lint:translations": {
      "command": "babel bin/lint-translations.js | node"
    },
    "start": {
      "command": "node bin/dev-server",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "test": {
      "command": "node ./node_modules/karma/bin/karma start config/karma.config",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    },
    "tx:push": {
      "command": "babel-node bin/tx.js push"
    },
    "tx:pull": {
      "command": "babel-node bin/tx.js pull"
    }
  }
}
